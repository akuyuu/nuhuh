{
	"manifest_version": 3,
	"name": "nuhuh",
	"version": "1.0",
	"description": "Removes twitter (currently x) communities from your timeline",
	"permissions": ["activeTab", "scripting", "storage"],
	"host_permissions": ["*://twitter.com/*", "*://x.com/*"],
	"action": {
		"default_popup": "popup.html"
	}
}
